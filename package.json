{
  "name": "dool-build",
  "version": "2.1.1",
  "description": "d-band tool & dev tool",
  "bin": {
    "dool-build": "cli.js"
  },
  "scripts": {
    "lint": "eslint --ext .js src test/*.test.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test nyc mocha --no-timeouts",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "atpl-loader": "^0.2.0",
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.22.0",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "^1.3.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "commander": "^2.9.0",
    "css-loader": "^0.28.0",
    "es6-promise": "^4.0.5",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.0",
    "fork-pool": "^2.0.0",
    "glob": "^7.1.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^4.0.0",
    "map-json-webpack-plugin": "^1.1.0",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/dool-build.git"
  },
  "keywords": [
    "d-band",
    "dev",
    "tool",
    "dool",
    "dool-build"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/dool-build/issues"
  },
  "homepage": "https://github.com/d-band/dool-build#readme",
  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^8.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.1.0",
    "nyc": "^10.0.0",
    "object-assign": "^4.1.0",
    "pre-commit": "^1.2.2",
    "webpack-md5-hash": "^0.0.5"
  }
}
