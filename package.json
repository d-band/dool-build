{
  "name": "dool-build",
  "version": "0.0.17",
  "description": "d-band tool & dev tool",
  "bin": {
    "dool-build": "cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "node --harmony node_modules/.bin/jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "atpl-loader": "~0.2.0",
    "autoprefixer": "~6.3.2",
    "babel-core": "~6.5.1",
    "babel-core-resolve-enhance": "~0.1.2",
    "babel-loader": "~6.2.2",
    "babel-preset-es2015": "~6.5.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "commander": "~2.9.0",
    "css-loader": "~0.23.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "gauge": "~1.2.5",
    "glob": "~7.0.0",
    "harmonize": "~1.4.4",
    "json-loader": "~0.5.4",
    "less": "~2.6.0",
    "less-loader": "~2.2.2",
    "map-json-webpack-plugin": "~1.1.0",
    "postcss-loader": "~0.8.0",
    "rimraf": "~2.5.1",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/dool-build.git"
  },
  "keywords": [
    "d-band",
    "dev",
    "tool",
    "dool",
    "dool-build"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/dool-build/issues"
  },
  "homepage": "https://github.com/d-band/dool-build#readme",
  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-jest": "^6.0.1",
    "coveralls": "^2.11.6",
    "jest-cli": "^0.8.2",
    "object-assign": "^4.0.1",
    "webpack-md5-hash": "0.0.5"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "test",
    "testFileExtensions": [
      "test.js"
    ],
    "collectCoverage": true,
    "verbose": true
  }
}