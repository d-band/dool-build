  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "babel-cli": "~6.9.0",
    "babel-preset-es2015": "~6.6.0",
    "chai": "~3.5.0",
    "coveralls": "~2.11.9",
    "istanbul": "~0.4.3",
    "mocha": "~2.4.5",
    "object-assign": "~4.1.0",
